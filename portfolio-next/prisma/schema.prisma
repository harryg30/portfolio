
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Station {
  id             Int             @id @default(autoincrement())
  number         String
  name           String
  latitude       Float
  longitude      Float
  district       String
  public         Boolean
  totalDocks     Int
  deploymentYear Int
  partOfRide     String
  stationOnRide  StationOnRide[]
}

model Ride {
  id            Int             @id @default(autoincrement())
  rideId        String
  startTime     String
  endedTime     String
  member        Boolean
  stationOnRide StationOnRide[]
}

model StationOnRide {
  ride      Ride    @relation(fields: [rideId], references: [id])
  rideId    Int
  station   Station @relation(fields: [stationId], references: [id])
  stationId Int
  wasStart  Boolean
  @@id([rideId, stationId])
}